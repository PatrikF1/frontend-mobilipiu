{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://backend-mobilipiu.onrender.com/api",
    "VITE_BACKEND_URL": "https://backend-mobilipiu.onrender.com"
  }
} 